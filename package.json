{
  "name": "clt-attr",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9000",
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "joi": "^13.6.0",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.5",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "npm-run-all": "^4.1.3",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "recompose": "^0.30.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start --source-map true",
    "start": "npm-run-all -p watch-css start-js",
    "watch:server": "nodemon",
    "run:server": "ts-node -r tsconfig-paths/register ./src/server/server.ts",
    "debug": "node --inspect=5858 -r ts-node/register -r tsconfig-paths/register src/server/server.ts",
    "debug:no-inspect": "node -r ts-node/register -r tsconfig-paths/register src/server/server.ts",
    "watch:debug": "nodemon --exec ts-node --inspect=5858 -r tsconfig-paths/register src/server/server.ts",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/classnames": "^2.2.6",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/joi": "^13.4.4",
    "@types/material-ui": "^0.21.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.9.4",
    "@types/ramda": "^0.25.37",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/recompose": "^0.26.4",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^3.0.3"
  }
}